#include<stdio.h>
void main()
{
	int i,ar1[10],larnum,sum[10];
	printf("Enter first array:-\n");
	for(i=0;i<=9;++i)
	{
		printf("ar1[%d]=",i);	
		scanf("%d",&ar1[i]);
	}
  larnum = ar1[0];
  for(i=0;i<=9;++i)
	{
		if(ar1[i]>larnum){
      larnum = ar1[i];
    }
    
    
	}
   printf("%d",larnum);

	
	
